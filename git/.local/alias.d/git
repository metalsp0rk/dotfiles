alias \
    gpuo="git push -u origin \$(git rev-parse --abbrev-ref HEAD)" \
    gcyolo="git commit -m \"\$(curl https://whatthecommit.com/index.txt)\"" \
    gus="export CD=\$PWD; cd \$(git rev-parse --show-toplevel); git restore --staged \$(git diff --name-only --cached); cd \$CD" \
    gsur="git submodule update --recursive" \
    gclb="git clone --bare " \
    gbrc="git branch --remote | cat" \
    gpuof="gpuo --force" \
